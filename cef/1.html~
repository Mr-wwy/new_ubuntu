<script>
function myFunction(){
	document.getElementById('demo').innerHTML='Hello Web';
}

function myFunction2(){
	document.getElementById("demo").innerHTML="我又来了 JavaScript";

function getrandom(int min, int max){
	return Math.floor(Math.random() * (max-min)) + min;
}
	document.getElementById('demo').innerHTML=getrandom(1,10);
}
</script>

<button onmouseover='myFunction()'>点击我！</button>
<input type="button" value="试一试" onclick="myFunction2()"/>
<input type="button" value="还原" onclick="myFunction3()"/>
<button onclick="myFunction4()">看看速度</button>

<input type='button' value='外部js' onclick='myFunction5()'/>
<input type='button' value='随机数' onclick='getrandom(1,10)'/>

